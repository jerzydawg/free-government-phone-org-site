{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/:path*"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(?<city>[a-z0-9-]+)-(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/:city/"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<city>[a-z0-9-]+)-(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/:city/:path*"
    }
  ]
}
